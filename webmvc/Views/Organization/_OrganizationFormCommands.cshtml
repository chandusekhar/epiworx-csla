@model Epiworx.WebMvc.Models.OrganizationFormModel
@using Epiworx.WebMvc.Helpers
<ul class="commands">
    <li>
        <button>
            Save Changes</button></li>
    <li><span>or</span></li>
    @if (!Model.Organization.IsNew)
    { 
        <li>@Html.ActionLink("Delete", "Delete", new { id = Model.Organization.OrganizationId })</li>
    }
    <li><a href="@if (Model.Organization.IsNew) {<text>@Url.Action("Index", "Home")</text> } else {<text>@Url.Action("Details", new { id = Model.Organization.OrganizationId})</text>}">Back</a></li>
</ul>
