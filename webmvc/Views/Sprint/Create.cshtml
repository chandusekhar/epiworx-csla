@model Epiworx.WebMvc.Models.SprintFormModel
@using Epiworx.WebMvc.Helpers
<div class="hgroup">
    <img src="@Url.Content("~/Content/sprint.png")" alt="Sprint"/>
    <h2>
        New Sprint</h2>
</div>
@Html.ValidationSummary(false, "Oh snap! Looks like we ran into some trouble. Please correct the errors and try again.")
@using (Html.BeginForm(new { projectId = Model.Sprint.ProjectId }))
{
    @Html.Partial("_SprintForm")
    @Html.Partial("_SprintFormCommands")
}