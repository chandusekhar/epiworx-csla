@model Epiworx.WebMvc.Models.ProjectFormModel
@using Epiworx.WebMvc.Helpers
<div class="part">
    <div class="wrapper">
        <div class="hgroup">
            <img src="@Url.Content("~/Content/user.png")" alt="User"/>
            <h3>
                Collaborators</h3>
        </div>
        <ul class="collaborators">
            @foreach (var user in Model.Users)
            {
                <li>@Html.Gravatar(user.UserEmail, user.UserName)@user.UserName@Html.ActionLink("Delete", "Delete", "ProjectUser", new { id = user.ProjectUserMemberId }, new { @class = "action" })</li>
            }
        </ul>
    </div>
</div>
