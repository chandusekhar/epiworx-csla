@model Epiworx.WebMvc.Models.ProjectFormModel
@using Epiworx.WebMvc.Helpers
<ul class="commands">
    <li>
        <button>
            Save Changes</button></li>
    <li><span>or</span></li>
    @if (!Model.Project.IsNew)
    { 
        <li>@Html.ActionLink("Delete", "Delete", new { id = Model.Project.ProjectId, returnUrl = Model.ReturnUrl }, new { @class = "delete" })</li>
    }
    <li><a href="@Server.UrlDecode(Model.ReturnUrl)">Back</a></li>
</div> 