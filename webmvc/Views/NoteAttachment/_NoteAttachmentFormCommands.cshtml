@model Epiworx.WebMvc.Models.AttachmentFormModel
@using Epiworx.WebMvc.Helpers
<ul class="commands">
    <li>
        <button>
            Save Changes</button></li>
    <li><span>or</span></li>
    @if (!Model.Attachment.IsNew)
    { 
        <li>@Html.ActionLink("Delete", "Delete", new { id = Model.Attachment.AttachmentId })</li>
    }
    <li><a href="@Url.Action("Details", Model.Attachment.SourceTypeName, new { id = Model.Attachment.SourceId })">Back</a></li>
</ul>
